{
  "name": "backend",
  "version": "0.1.0",
  "description": "Minimal NestJS backend for biobank project",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "postbuild": "cp -r src/templates dist/ || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "express": "latest",
    "neo4j-driver": "latest",
    "@supabase/supabase-js": "latest",
    "@nestjs/graphql": "latest",
    "@nestjs/core": "^11.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "latest",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "supertest": "^6.3.0",
    "jest-coverage-badges": "^1.1.2"
  }
}